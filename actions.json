{
    "manifest": {
        "displayName": "Display Image",
        "invocationName": "Display Image",
        "category": "PRODUCTIVITY"
    },
    "actions": [
        {
            "name": "DisplayImage",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.example.intents.DisplayImage",
                "parameters": [
                  {
                      "name":"image",
                      "type":"Image"
                  }
                ],
                "trigger": {
                    "queryPatterns": [
                        "Draw a $Image:image",
                        "$Image:image screen",
                        "Draw $Image:image"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Here you go"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "DisplayImage",
                                    "params": {
                                      "image":"$image"
                                    }
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "DisplayCategories",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.example.intents.DisplayCategories",
                "trigger": {
                    "queryPatterns": [
                        "What can you draw?"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "I can draw a few characters, animals, logos, food, and Roman. Don't get picky!"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "DisplayCategories"
                                }
                            }
                        ]
                    }
                }
            }
        },
        {
            "name": "DisplayRandomImage",
            "availability": {
                "deviceClasses": [
                    {
                        "assistantSdkDevice": {}
                    }
                ]
            },
            "intent": {
                "name": "com.example.intents.DisplayRandomImage",
                "trigger": {
                    "queryPatterns": [
                        "Draw a random image"
                    ]
                }
            },
            "fulfillment": {
                "staticFulfillment": {
                    "templatedResponse": {
                        "items": [
                            {
                                "simpleResponse": {
                                    "textToSpeech": "Here is your random image"
                                }
                            },
                            {
                                "deviceExecution": {
                                    "command": "DisplayRandomImage"
                                }
                            }
                        ]
                    }
                }
            }
        }
    ],
